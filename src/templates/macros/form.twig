{% macro render(form) %}

    {% import 'macros/htmlElement.twig' as htmlElementMacro %}

    {{ htmlElementMacro.open('form', form.getAttributes) }}

        {% if form.getError %}
            <div class="generalFormError">{{ form.getErrorMessage }}</div>
        {% endif %}

        {% import 'macros/nodes.twig' as nodesMacro %}

        {{ nodesMacro.renderRecursive(form.getNodes) }}

    {{ htmlElementMacro.close('form') }}

{% endmacro %}